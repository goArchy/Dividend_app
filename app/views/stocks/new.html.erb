<% provide(:title, 'Add Stock') %>
<h1>Add Stock</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@stock) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :ticker %>
      <%= f.text_field :ticker %>

      <%= f.submit "Add Company", class: "btn btn-large btn-primary" %>
    <% end %>
      <%= link_to "Back",   '#' %>
  </div>
</div>
